{
  "name": "global-view",
  "version": "0.2.0",
  "description": "NPM distribution of GlobalView.js library.",
  "main": "lib/index.js",
  "module": "es/index.js",
  "repository": {
    "type": "git",
    "url": "https://github.com/RcSepp/GlobalView.js.git"
  },
  "keywords": [
    "visualization",
    "scatterplot",
    "webgl"
  ],
  "browser": "child_process: false, dist/global-view.js",
  "scripts": {
    "build": "npm run transpile:commonjs && npm run transpile:es && npm run bundle",
    "bundle": "cross-env BABEL_ENV=es NODE_ENV=production webpack --config webpack.config.js",
    "clean": "rimraf es lib dist",
    "lint": "eslint src/",
    "preversion": "npm run clean && npm run lint && npm run test",
    "postversion": "git push && git push --tags",
    "transpile:commonjs": "cross-env BABEL_ENV=commonjs babel src --out-dir lib",
    "transpile:es": "cross-env BABEL_ENV=es babel src --out-dir es",
    "test": "cross-env BABEL_ENV=test mocha --require babel-polyfill --require babel-register src/**/*.test.js",
    "test:coverage": "cross-env BABEL_ENV=test nyc --reporter=lcov --reporter=text npm test",
    "version": "npm run build && git add .",
    "start": "webpack-dev-server --config ./examples/webpack.examples.config.js",
    "examples": "webpack --config ./examples/webpack.examples.config.js && npm start",
    "start.beefy": "beefy --open",
    "examples.b": "npm run example1.b && npm run example2.b && npm run example3.b && npm run example4.b && npm run example5.b && npm run example6.b && npm run example7.b && npm run example8.b",
    "example1.b": "browserify examples/example1.js -o examples/example1.min.js",
    "example2.b": "browserify examples/example2.js -o examples/example2.min.js",
    "example3.b": "browserify examples/example3.js -o examples/example3.min.js",
    "example4.b": "browserify examples/example4.js -o examples/example4.min.js",
    "example5.b": "browserify examples/example5.js -o examples/example5.min.js",
    "example6.b": "browserify examples/example6.js -o examples/example6.min.js",
    "example7.b": "browserify examples/example7.js -o examples/example7.min.js",
    "example8.b": "browserify examples/example8.js -o examples/example8.min.js",
    "testall.b": "npm run build && npm run examples.b && npm start.beefy",
    "TODO_minify": "(TODO): cat bundle.js | uglifyjs -o bundle.js",
    "nibble": "eslint-nibble src/ examples/"
  },
  "files": [
    "es",
    "lib",
    "dist",
    "README.md",
    "package.json"
  ],
  "author": "",
  "license": "MIT",
  "devDependencies": {
    "babel-cli": "~6.26",
    "babel-core": "~6.26",
    "babel-loader": "~7.1",
    "babel-plugin-istanbul": "~4.1",
    "babel-polyfill": "~6.26",
    "babel-preset-env": "~1.6",
    "babel-register": "~6.26",
    "beefy": "^2.1.8",
    "browserify": "^15.1.0",
    "chai": "~4.1",
    "cpx": "^1.5.0",
    "cross-env": "^5.1.3",
    "eslint": "^4.15.0",
    "eslint-config-airbnb": "^16.1.0",
    "eslint-config-airbnb-base": "~12.1",
    "eslint-plugin-import": "^2.8.0",
    "eslint-plugin-react": "^7.5.1",
    "get-pixels": "^3.3.0",
    "gl": "^4.0.4",
    "image-diff": "^2.0.0",
    "install": "^0.10.2",
    "mocha": "~4.0",
    "ndarray": "^1.0.18",
    "npm": "^5.6.0",
    "nyc": "~11.4",
    "rimraf": "~2.6",
    "save-pixels": "^2.3.4",
    "tape": "^4.8.0",
    "webgl-to-img-stream": "^1.0.0",
    "webpack": "~3.10",
    "webpack-dev-server": "^2.11.1",
    "eslint-nibble": "^4.2.1",
    "eslint-plugin-jsx-a11y": "^6.0.3"
  },
  "dependencies": {
    "domready": "^1.0.8",
    "gl-matrix": "^2.4.0",
    "jquery": "^3.2.1",
    "jquery-csv": "^0.8.3",
    "jszip": "^3.1.5",
    "paralleljs": "^0.2.1"
  }
}
