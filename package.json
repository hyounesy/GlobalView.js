{
  "name": "global-view",
  "version": "0.1.0",
  "description": "GlobalView NPM package.",
  "main": "lib/index.js",
  "module": "es/index.js",
  "browser": "dist/template-package.js",
  "scripts": {
    "build": "npm run transpile:commonjs && npm run transpile:es && npm run bundle",
    "bundle": "cross-env BABEL_ENV=es NODE_ENV=production webpack --config webpack.config.js",
    "clean": "rimraf es lib dist",
    "lint": "eslint src/",
    "preversion": "npm run clean && npm run lint && npm run test",
    "postversion": "git push && git push --tags",
    "transpile:commonjs": "cross-env BABEL_ENV=commonjs babel src --out-dir lib && cpx \"src_lib/*\" lib",
    "transpile:es": "cross-env BABEL_ENV=es babel src --out-dir es",
    "test": "cross-env BABEL_ENV=test mocha --require babel-polyfill --require babel-register src/**/*.test.js",
    "test:coverage": "cross-env BABEL_ENV=test nyc --reporter=lcov --reporter=text npm test",
    "version": "npm run build && git add .",
    "start": "beefy --open",
    "start2": "beefy examples/example2.js --open",
    "examples": "npm run example1 && npm run example2 && npm run example3 && npm run example4 && npm run example5 && npm run example6",
    "example1": "browserify examples/example1.js -o examples/example1.min.js",
    "example2": "browserify examples/example2.js -o examples/example2.min.js",
    "example3": "browserify examples/example3.js -o examples/example3.min.js",
    "example4": "browserify examples/example4.js -o examples/example4.min.js",
    "example5": "browserify examples/example5.js -o examples/example5.min.js",
    "example6": "browserify examples/example6.js -o examples/example6.min.js",
    "TODO_minify": "(TODO): cat bundle.js | uglifyjs -o bundle.js"
  },
  "files": [
    "es",
    "lib",
    "dist",
    "README.md",
    "package.json"
  ],
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "babel-cli": "~6.26",
    "babel-core": "~6.26",
    "babel-loader": "~7.1",
    "babel-plugin-istanbul": "~4.1",
    "babel-polyfill": "~6.26",
    "babel-preset-env": "~1.6",
    "babel-register": "~6.26",
    "beefy": "^2.1.8",
    "browserify": "^15.1.0",
    "chai": "~4.1",
    "cpx": "^1.5.0",
    "cross-env": "^5.1.3",
    "eslint": "^4.15.0",
    "eslint-config-airbnb": "^16.1.0",
    "eslint-config-airbnb-base": "~12.1",
    "eslint-plugin-import": "^2.8.0",
    "eslint-plugin-react": "^7.5.1",
    "get-pixels": "^3.3.0",
    "gl": "^4.0.4",
    "image-diff": "^2.0.0",
    "install": "^0.10.2",
    "mocha": "~4.0",
    "ndarray": "^1.0.18",
    "npm": "^5.6.0",
    "nyc": "~11.4",
    "rimraf": "~2.6",
    "save-pixels": "^2.3.4",
    "tape": "^4.8.0",
    "webgl-to-img-stream": "^1.0.0",
    "webpack": "~3.10"
  },
  "dependencies": {
    "domready": "^1.0.8",
    "gl-matrix": "^2.4.0",
    "jquery-csv": "^0.8.3",
    "jszip": "^3.1.5",
    "parallel": "^1.2.0"
  }
}
